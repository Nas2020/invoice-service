# 1. Create profile
curl -X POST http://localhost:9000/api/profile \
  -H "Content-Type: application/json" \
  -d '{
    "business_name": "NAS TIL",
    "business_role": "Software Engineer",
    "business_address": "117 Eltomo Pvt",
    "business_city": "Limoges, ON- K0A 2M0",
    "business_phone": "4163000491",
    "business_gst": "70434 3953 RT0001"
  }'



  # 2. Verify profile was created
curl http://localhost:9000/api/profile

# 3. Create organization
curl -X POST http://localhost:9000/api/organizations \
  -H "Content-Type: application/json" \
  -d '{
    "company_name": "Rocket Doctor Inc.",
    "address": "180 John Street",
    "city": "Toronto ON",
    "postal_code": "M5T 1X5 Canada",
    "hourly_rate": 65,
    "tax_rate": 13,
    "business_role": "Senior Software Engineer"
  }'

  curl -X POST http://localhost:9000/api/organizations \
  -H "Content-Type: application/json" \
  -d '{
    "company_name": "VeriDID Corp.",
    "address": "246 Vaughan Rd",
